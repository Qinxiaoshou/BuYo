<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aode.buyoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity android:name=".StartActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".business.activity.LoginActivity" />
        <activity android:name=".business.activity.RegisterActivity"></activity>

        <activity android:name=".qinxiaoshou.activity.BusinessAddNewProductActivity"/>
        <activity android:name=".qinxiaoshou.activity.BusinessChooseBusinessAndPermissionActivity"/>
        <activity android:name=".qinxiaoshou.activity.BusinessMamageProductActivity"/>
        <activity android:name=".qinxiaoshou.activity.BusinessUpdateProductMessagesActivity"/>
        <activity android:name=".qinxiaoshou.activity.ConsumerOrderDetailActivity"/>
        <activity android:name=".qinxiaoshou.activity.ConsumerProductDetailsActivity"/>
    </application>

</manifest>
