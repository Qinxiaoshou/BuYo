<resources>







    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="notitle"  parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimary">#f5f5f5</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">#ff4d00</item>
    </style>


    <style name="common_button_style">
        <item name="android:layout_width">75dp</item>
        <item name="android:layout_height">34dp</item>
        <item name="android:background">@drawable/selector_red_corners_button</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">#3a3d3d</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="TextView_01">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">12dp</item>
        <!--<item name="android:background">@drawable/flag_03</item>-->
        <item name="android:background">@color/white</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="rl_Search">
       <item name="android:background">@drawable/flag_04</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    
    <style name="browser_business_btn">
        <item name="android:textSize">@dimen/x13</item>
        <item name="android:paddingLeft">17.5dp</item>
        <item name="android:paddingRight">17.5dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="home_bupi_imageview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">11.5dp</item>
    </style>
    <style name="home_bupi_column">
        <item name="android:clickable">true</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:background">@drawable/shape_border</item>
    </style>

    <style name="home_bupi_biaoti">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginTop">17dp</item>
        <item name="android:paddingLeft">7dp</item>
        <item name="android:textSize">@dimen/x16</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="home_bupi_detail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">13.5dp</item>
        <item name="android:paddingLeft">7dp</item>
        <item name="android:textColor">@color/huise3</item>
        <item name="android:textSize">@dimen/x12</item>
    </style>

    <style name="home_bupi_right_imageview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>

    <style name="home_remai_biaoti">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/x12</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="home_remai_detail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">@color/huise3</item>
        <item name="android:textSize">@dimen/x11</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="home_xinwen_detail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginLeft">3.5dp</item>
        <item name="android:textColor">@color/light_black</item>
        <item name="android:textSize">@dimen/x16</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="home_xinwen_yuandian">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentStart">true</item>
    </style>

    <style name="home_xinwen_huidian">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>
    
    <style name="transverse_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:background">@color/light_huise</item>
    </style>

    <style name="browser_business_changshang">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/x17</item>
    </style>

    <style name="browser_business_changshang_detail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">11.5dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/x14</item>
    </style>

    <style name="browser_business_changshang_detail2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">11.5dp</item>
        <item name="android:textColor">@color/huise4</item>
        <item name="android:textSize">@dimen/x13</item>
    </style>

    <style name="browser_business_changshang_fenshu">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/x15</item>
    </style>
</resources>
